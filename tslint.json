{
  "extends": "tslint-config-airbnb",
  "linterOptions": {
    "exclude": [
        "www",
        "node_modules",
        ".vscode",
        "platforms",
        "plugins"
    ]
  },
  "rules": {
    "no-duplicate-variable": true,
    "no-unused-variable": [true]
  },
  "rulesDirectory": ["node_modules/tslint-eslint-rules/dist/rules"]
}